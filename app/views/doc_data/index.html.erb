<p id="notice"><%= notice %></p>

<h1>Doc Data</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doc_data.each do |doc_datum| %>
      <tr>
        <td><%= link_to 'Show', doc_datum %></td>
        <td><%= link_to 'Edit', edit_doc_datum_path(doc_datum) %></td>
        <td><%= link_to 'Destroy', doc_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doc Datum', new_doc_datum_path %>
